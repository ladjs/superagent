{
  "extends": ["eslint:recommended", "plugin:compat/recommended"],
  "env": {
    "node": false,
    "browser": true,
    "amd": true,
    "es6": true
  },
  "plugins": ["compat"],
  "rules": {
    "node/no-unsupported-features/es-builtins": ["error", {
      "version": ">=8.0.0",
      "ignores": [
        "Atomics",
        "BigInt",
        "SharedArrayBuffer"
      ]
    }],
    "compat/compat": "error",
    "no-console": "off",
    "no-empty": "off",
    "no-extra-semi": "off",
    "no-func-assign": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-useless-escape": "off",
    "no-cond-assign": "off",
    "no-redeclare": "off",
    "node/no-exports-assign": "off",
    "no-unsafe-finally": "off",
    "complexity": ["error", 10000],
    "max-statements": "off",
    "no-constant-condition": "off",
    "no-control-regex": "off",
    "no-fallthrough": "off",
    "operator-linebreak": "off"
  },
  "globals": {
    "regeneratorRuntime": "writable"
  },
  "settings": {
    "polyfills": [
      "WeakRef",
      "BigInt"
    ]
  }
}
